{
  "version": 2,
  "builds": [
    {
      "src": "apps/angular-app/**",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/apps/angular-app"
      }
    },
    {
      "src": "apps/react-app/**",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/apps/react-app"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
